{
    "name": "innoge/laravel-msgraph-mail",
    "description": "Laravel Mail driver for Microsoft Office 365 using the MSGraph API",
    "keywords": [
        "InnoGE",
        "laravel",
        "laravel-msgraph-mail"
    ],
    "homepage": "https://github.com/innoge/laravel-msgraph-mail",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Geisendoerfer",
            "email": "geisi@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.12.1",
        "illuminate/contracts": "^8.83.27",
        "symfony/mailer": "^5.4.45"
    },
    "require-dev": {
        "composer/semver": "^3.4",
        "guzzlehttp/guzzle": "^7.5",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "InnoGE\\LaravelMsGraphMail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InnoGE\\LaravelMsGraphMail\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "@php vendor/bin/phpstan analyse",
        "test": "@php vendor/bin/pest",
        "test-coverage": "@php vendor/bin/pest --coverage",
        "format": "@php vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InnoGE\\LaravelMsGraphMail\\LaravelMsGraphMailServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
